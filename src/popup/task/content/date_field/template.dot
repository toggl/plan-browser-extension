<div class="{{=it.css.container}} row row--align-center">
  <div class="{{=it.css.label}} text--color-new-gray row row--align-center">
    <span class="{{=it.css.icon}} {{=it.css.iconsvg}} icon icon-svg icon-svg--14" data-hook="heading-icon"></span>
    From - to
  </div>

  <div data-hook="input" class="row row--align-center pointer relative row--grow task-form__field-input-container">
    <div class="{{=it.css.start}}" data-hook="input-start-date" tabindex="6"></div>
    <div class="{{=it.css.divider}}">-</div>
    <div class="{{=it.css.end}}">
      <div data-hook="input-end-date" tabindex="7"></div>
    </div>
    <span class="task-form__readonly {{=it.css.readonly}} icon-svg"></span>
  </div>
</div>
